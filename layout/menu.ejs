<nav id="menu">

    <div class="desktop-menu">
        <a href="/">
            <span class="title"><%- config.title %></span>
        </a>
        <% Object.keys(theme.menu).forEach(key => { %>
        <a href="<%=theme.menu[key].src%>">
            <span>
                <ion-icon name="<%=theme.menu[key].icon %>"></ion-icon>
            </span>
            <span><%=key%></span>
        </a>
        <% }); %>
    </div>

    <div class="phone-menu">

    </div>
</nav>